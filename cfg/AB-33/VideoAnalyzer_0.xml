<?xml version="1.0" encoding="gb2312" ?>
<configure>
    <golbal>
        <LocalAddr>0.0.0.0</LocalAddr>
        <UDPServer>192.168.1.74:10116</UDPServer>
    </golbal>
    <camera channel="0">
        <ParamStruct>
            <iStyleChange value="0" />
            <!-- 0_natural 1_tree 2_curver 3_-->
            <bSensitiveFlag value="1" />
            <!-- 0_灵敏度低 1_灵敏度高 -->
            <bTransLensImage value="0" />
            <!-- 0_正常 1_镜像 -->
            <bBinarizeSubThreshold value="40" />
            <!-- 二值化阈值 (30 ~ 50) -->
            <bNightSubThreshold value="60" />
            <!-- 夜间二值化阈值 (40 ~ 70) -->
            <iImfilterSingleThreshold value="1" />
            <!-- 去小点阈值 (1 ~ 5) -->
            <iImdilateThreshold value="2" />
            <!-- 膨胀阈值 (1 ~ 5) -->
            <iWhiteSpotNumMax value="1000" />
            <!-- 最大白点数 (500 ~ 2000) -->
            <iWhiteSpotNumMin value="20" />
            <!-- 最小白点数 (1 ~ 50) -->
            <iXTrackContinueThreshold value="1" />
            <!-- 判断目标改变方向的阈值 (0 ~ 3) -->
            <iXTrackOffsetValue value="0" />
            <!-- 判断条件:保证x方向有一定偏差 (0 ~ 3) -->
            <iTrackMaxFrameNum value="25" />
            <!-- 跟踪最大帧数 (20 ~ 39) -->
            <iLittleRegionValue value="10" />
            <!-- 小区域中Y的最高点 (5 ~ 285) -->
            <iNightRangeVal value="120" />
            <!-- 判读是否到夜间的阈值 (80 ~ 150) -->
            <iCurveLeftVal value="0" />
            <!-- 拟合曲线最左边的点 (0 ~ 300) -->
            <iCurveRightVal value="450" />
            <!-- 拟合曲线最右边的点 (0 ~ 500) -->
            <iPersonFlag value="1" />
            <!-- 人员检测标志 (0 ~ 1) -->
            <iPersonWhitePotNum value="80" />
            <!-- 人员检测中人的白点数 (5 ~ 285) -->
            <iAlarmDelay value="1" />
            <!-- 报警延时 (20 ~ 750) -->
            <iSceneDepth value="0" />
            <!-- 景深 (0 ~ 3) -->
            <tRectLittleRegion>
                <!-- 小区域范围 -->
                <LineSet bFlag="1" BeginPointX="88" BeginPointY="5" EndPointX="136" EndPointY="48" />
                <!-- 小区域范围 -->
            </tRectLittleRegion>
            <tLittleRegionYLine>
                <LineSet bFlag="1" BeginPointX="98" BeginPointY="18" EndPointX="135" EndPointY="20" />
                <!-- 小区域的高度线 -->
            </tLittleRegionYLine>
            <tLineCurverRange>
                <LineSet bFlag="1" BeginPointX="131" BeginPointY="38" EndPointX="266" EndPointY="256" />
                <!-- 拟合曲线范围 -->
            </tLineCurverRange>
            <tLineStraightFirst>
                <LineSet bFlag="1" BeginPointX="113" BeginPointY="51" EndPointX="187" EndPointY="270" />
                <!-- 两条轨迹经过区域线中的第一条线 -->
            </tLineStraightFirst>
            <tLineStraightSecond>
                <LineSet bFlag="1" BeginPointX="144" BeginPointY="20" EndPointX="335" EndPointY="222" />
                <!-- 两条轨迹经过区域线中的第二条线 -->
            </tLineStraightSecond>
            <tLineBlackLeft>
                <LineSet bFlag="1" BeginPointX="70" BeginPointY="7" EndPointX="9" EndPointY="208" />
                <!-- 加入的黑色直线中的左边为黑  -->
            </tLineBlackLeft>
            <tLineBlackRight>
                <LineSet bFlag="1" BeginPointX="314" BeginPointY="5" EndPointX="338" EndPointY="18" />
                <!-- 加入的黑色直线中的右边为黑  -->
            </tLineBlackRight>
            <tRectBlackBlock>
                <LineSet bFlag="1" BeginPointX="156" BeginPointY="177" EndPointX="201" EndPointY="273" />
                <!-- 加入的黑色块范围  -->
                <LineSet bFlag="1" BeginPointX="219" BeginPointY="217" EndPointX="244" EndPointY="265" />
                <!-- 加入的黑色块范围  -->
                <LineSet bFlag="0" BeginPointX="0" BeginPointY="0" EndPointX="0" EndPointY="0" />
                <!-- 加入的黑色块范围  -->
                <LineSet bFlag="0" BeginPointX="0" BeginPointY="0" EndPointX="0" EndPointY="0" />
                <!-- 加入的黑色块范围  -->
                <LineSet bFlag="0" BeginPointX="0" BeginPointY="0" EndPointX="0" EndPointY="0" />
                <!-- 加入的黑色块范围  -->
            </tRectBlackBlock>
            <tNightRange>
                <LineSet bFlag="1" BeginPointX="13" BeginPointY="6" EndPointX="42" EndPointY="43" />
                <!-- 小区域范围 -->
                <LineSet bFlag="1" BeginPointX="314" BeginPointY="14" EndPointX="339" EndPointY="41" />
                <!-- 小区域范围 -->
            </tNightRange>
            <iPersonRange>
                <LineSet bFlag="1" BeginPointX="90" BeginPointY="52" EndPointX="15" EndPointY="266" />
                <!--  -->
                <LineSet bFlag="1" BeginPointX="132" BeginPointY="41" EndPointX="145" EndPointY="272" />
                <!--  -->
                <LineSet bFlag="1" BeginPointX="103" BeginPointY="30" EndPointX="133" EndPointY="30" />
                <!--  -->
            </iPersonRange>
            <iPersonTopVal value="1" />
        </ParamStruct>
        <ParamDistinguish>
            <TrackNumMax FirstValue="11" SecondValue="18" ThirdValue="25" FouthValue="14" />
            <!-- 判断跟踪次数的大值 -->
            <TrackNumMin FirstValue="5" SecondValue="7" ThirdValue="12" FouthValue="1" />
            <!-- 判断跟踪次数的小值 -->
            <LostNumVal FirstValue="0" SecondValue="0" ThirdValue="0" FouthValue="0" />
            <!-- 判断的丢帧数 -->
            <MigrationDiff FirstValue="1" SecondValue="1" ThirdValue="1" FouthValue="3" />
            <!-- 判断X每帧偏移量 -->
            <OriginCurrDis FirstValue="10" SecondValue="20" ThirdValue="30" FouthValue="1" />
            <!-- 判断原始点到当前点的X距离 -->
            <YHeightVal FirstValue="15" SecondValue="1" ThirdValue="1" FouthValue="1" />
            <!-- 判断Y方向的最大距离 -->
            <XContValue FirstValue="4" SecondValue="6" ThirdValue="8" FouthValue="1" />
            <!-- 判断X方向的同一方向运动数 -->
            <XMatchValue FirstValue="3" SecondValue="5" ThirdValue="1" FouthValue="1" />
            <!-- 判断X方向的与预测的匹配个数 -->
            <YMatchValue FirstValue="2" SecondValue="5" ThirdValue="1" FouthValue="1" />
            <!-- 判断Y方向的与预测的匹配个数 -->
            <FindObjValue FirstValue="6" SecondValue="10" ThirdValue="12" FouthValue="6" />
            <!-- 判断找到目标个数 -->
            <RiseNumVal FirstValue="3" SecondValue="4" ThirdValue="4" FouthValue="4" />
            <!-- 判断上升帧数 -->
            <DownNumVal FirstValue="3" SecondValue="4" ThirdValue="4" FouthValue="4" />
            <!-- 判断下降帧数 -->
            <FindObjValMax FirstValue="1" SecondValue="1" ThirdValue="1" FouthValue="1" />
            <!-- 判断找到目标的大值 -->
            <FindObjValMin FirstValue="1" SecondValue="1" ThirdValue="1" FouthValue="1" />
            <!-- 判断找到目标目标的小值-->
            <WhiteSpotVal FirstValue="200" SecondValue="400" ThirdValue="600" FouthValue="1" />
            <!-- 判断白点数3个区段 -->
            <TopPointVal FirstValue="15" SecondValue="1" ThirdValue="1" FouthValue="1" />
            <!-- 判断Y方向的最大值 -->
            <fFindObjInverse FirstValue="0.600000" SecondValue="0.700000" ThirdValue="0.700000" FouthValue="0.700000" />
            <!-- 判断找到目标和跟踪数的比率 -->
            <fXMatchInverse FirstValue="0.400000" SecondValue="0.400000" ThirdValue="0.300000" FouthValue="0.700000" />
            <!-- 判断 X匹配数与跟踪数的比率 -->
            <fYMatchInverse FirstValue="0.300000" SecondValue="0.300000" ThirdValue="0.200000" FouthValue="0.700000" />
            <!-- 判断 Y匹配数与跟踪数的比率 -->
            <fXContInverse FirstValue="0.500000" SecondValue="0.500000" ThirdValue="0.500000" FouthValue="1.000000" />
            <!-- 判断 X同一方向数与跟踪数的比率 -->
        </ParamDistinguish>
    </camera>
</configure>
