<?xml version="1.0" encoding="gb2312" ?>
<configure>
    <golbal>
        <LocalAddr>0.0.0.0</LocalAddr>
        <UDPServer>10.2.1.13:10116</UDPServer>
        <HeartBeat>180</HeartBeat>
        <!-- 发送心跳消息的间隔时间(秒) -->
        <ImagePath>d:\AlarmImage</ImagePath>
        <Debug>1</Debug>
    </golbal>
    <camera channel="0">
        <PersonDetect>
            <WarningLine>
                <Point x="19" y="188" />
                <Point x="281" y="110" />
            </WarningLine>
            <Mask>
                <Poly>
                    <Point x="4" y="5" />
                    <Point x="7" y="57" />
                    <Point x="343" y="57" />
                    <Point x="348" y="2" />
                    <Point x="4" y="2" />
                </Poly>
            </Mask>
            <Night>
                <Rect x1="120" y1="3" x2="147" y2="23" />
                <Rect x1="222" y1="3" x2="239" y2="14" />
            </Night>
            <iNightRangeVal value="120" />
            <!-- 判读是否到夜间的阈值 (80 ~ 150) -->
            <iNigthRgbThreshold value="80" />
            <!-- 夜间二值化阈值 (60 ~ 90) -->
        </PersonDetect>
        <ParamStruct>
            <iStyleChange value="1" />
            <!-- 0_natural 1_tree 2_curver 3_-->
            <bSensitiveFlag value="1" />
            <!-- 0_灵敏度低 1_灵敏度高 -->
            <bTransLensImage value="0" />
            <!-- 0_正常 1_镜像 -->
            <bBinarizeSubThreshold value="40" />
            <!-- 二值化阈值 (30 ~ 50) -->
            <bNightSubThreshold value="60" />
            <!-- 夜间二值化阈值 (40 ~ 70) -->
            <iImfilterSingleThreshold value="1" />
            <!-- 去小点阈值 (1 ~ 5) -->
            <iImdilateThreshold value="2" />
            <!-- 膨胀阈值 (1 ~ 5) -->
            <iWhiteSpotNumMax value="1500" />
            <!-- 最大白点数 (500 ~ 2000) -->
            <iWhiteSpotNumMin value="20" />
            <!-- 最小白点数 (1 ~ 50) -->
            <iXTrackContinueThreshold value="1" />
            <!-- 判断目标改变方向的阈值 (0 ~ 3) -->
            <iXTrackOffsetValue value="0" />
            <!-- 判断条件:保证x方向有一定偏差 (0 ~ 3) -->
            <iTrackMaxFrameNum value="25" />
            <!-- 跟踪最大帧数 (20 ~ 39) -->
            <iLittleRegionValue value="10" />
            <!-- 小区域中Y的最高点 (5 ~ 285) -->
            <iNightRangeVal value="120" />
            <!-- 判读是否到夜间的阈值 (80 ~ 150) -->
            <iCurveLeftVal value="0" />
            <!-- 拟合曲线最左边的点 (0 ~ 300) -->
            <iCurveRightVal value="450" />
            <!-- 拟合曲线最右边的点 (0 ~ 500) -->
            <iPersonFlag value="1" />
            <!-- 人员检测标志 (0 ~ 1) -->
            <iPersonWhitePotNum value="80" />
            <!-- 人员检测中人的白点数 (5 ~ 285) -->
            <iAlarmDelay value="1" />
            <!-- 报警延时 (20 ~ 750) -->
            <iSceneDepth value="0" />
            <!-- 景深 (0 ~ 3) -->
            <tRectLittleRegion>
                <!-- 小区域范围 -->
                <LineSet bFlag="0" BeginPointX="0" BeginPointY="0" EndPointX="0" EndPointY="0" />
                <!-- 小区域范围 -->
            </tRectLittleRegion>
            <tLittleRegionYLine>
                <LineSet bFlag="0" BeginPointX="0" BeginPointY="0" EndPointX="0" EndPointY="0" />
                <!-- 小区域的高度线 -->
            </tLittleRegionYLine>
            <tLineCurverRange>
                <LineSet bFlag="1" BeginPointX="175" BeginPointY="60" EndPointX="324" EndPointY="163" />
                <!-- 拟合曲线范围 -->
            </tLineCurverRange>
            <tLineStraightFirst>
                <LineSet bFlag="1" BeginPointX="166" BeginPointY="55" EndPointX="313" EndPointY="218" />
                <!-- 两条轨迹经过区域线中的第一条线 -->
            </tLineStraightFirst>
            <tLineStraightSecond>
                <LineSet bFlag="1" BeginPointX="185" BeginPointY="37" EndPointX="328" EndPointY="149" />
                <!-- 两条轨迹经过区域线中的第二条线 -->
            </tLineStraightSecond>
            <tLineBlackLeft>
                <LineSet bFlag="1" BeginPointX="145" BeginPointY="5" EndPointX="38" EndPointY="280" />
                <!-- 加入的黑色直线中的左边为黑  -->
            </tLineBlackLeft>
            <tLineBlackRight>
                <LineSet bFlag="1" BeginPointX="192" BeginPointY="5" EndPointX="338" EndPointY="75" />
                <!-- 加入的黑色直线中的右边为黑  -->
            </tLineBlackRight>
            <tRectBlackBlock>
                <LineSet bFlag="0" BeginPointX="0" BeginPointY="0" EndPointX="0" EndPointY="0" />
                <!-- 加入的黑色块范围  -->
                <LineSet bFlag="0" BeginPointX="0" BeginPointY="0" EndPointX="0" EndPointY="0" />
                <!-- 加入的黑色块范围  -->
                <LineSet bFlag="0" BeginPointX="0" BeginPointY="0" EndPointX="0" EndPointY="0" />
                <!-- 加入的黑色块范围  -->
                <LineSet bFlag="0" BeginPointX="0" BeginPointY="0" EndPointX="0" EndPointY="0" />
                <!-- 加入的黑色块范围  -->
                <LineSet bFlag="0" BeginPointX="0" BeginPointY="0" EndPointX="0" EndPointY="0" />
                <!-- 加入的黑色块范围  -->
            </tRectBlackBlock>
            <tNightRange>
                <LineSet bFlag="1" BeginPointX="113" BeginPointY="3" EndPointX="158" EndPointY="23" />
                <!-- 小区域范围 -->
                <LineSet bFlag="1" BeginPointX="13" BeginPointY="25" EndPointX="35" EndPointY="47" />
                <!-- 小区域范围 -->
            </tNightRange>
            <iPersonRange>
                <LineSet bFlag="1" BeginPointX="130" BeginPointY="62" EndPointX="16" EndPointY="280" />
                <!--  -->
                <LineSet bFlag="1" BeginPointX="258" BeginPointY="43" EndPointX="346" EndPointY="287" />
                <!--  -->
                <LineSet bFlag="1" BeginPointX="142" BeginPointY="55" EndPointX="186" EndPointY="57" />
                <!--  -->
            </iPersonRange>
            <iPersonTopVal value="1" />
        </ParamStruct>
        <ParamDistinguish>
            <TrackNumMax FirstValue="11" SecondValue="18" ThirdValue="25" FouthValue="18" />
            <!-- 判断跟踪次数的大值 -->
            <TrackNumMin FirstValue="5" SecondValue="10" ThirdValue="14" FouthValue="1" />
            <!-- 判断跟踪次数的小值 -->
            <LostNumVal FirstValue="0" SecondValue="0" ThirdValue="0" FouthValue="2" />
            <!-- 判断的丢帧数 -->
            <MigrationDiff FirstValue="1" SecondValue="1" ThirdValue="1" FouthValue="3" />
            <!-- 判断X每帧偏移量 -->
            <OriginCurrDis FirstValue="10" SecondValue="20" ThirdValue="25" FouthValue="1" />
            <!-- 判断原始点到当前点的X距离 -->
            <YHeightVal FirstValue="15" SecondValue="1" ThirdValue="1" FouthValue="1" />
            <!-- 判断Y方向的最大距离 -->
            <XContValue FirstValue="4" SecondValue="5" ThirdValue="6" FouthValue="1" />
            <!-- 判断X方向的同一方向运动数 -->
            <XMatchValue FirstValue="3" SecondValue="5" ThirdValue="1" FouthValue="1" />
            <!-- 判断X方向的与预测的匹配个数 -->
            <YMatchValue FirstValue="2" SecondValue="5" ThirdValue="1" FouthValue="1" />
            <!-- 判断Y方向的与预测的匹配个数 -->
            <FindObjValue FirstValue="6" SecondValue="8" ThirdValue="10" FouthValue="5" />
            <!-- 判断找到目标个数 -->
            <RiseNumVal FirstValue="3" SecondValue="4" ThirdValue="4" FouthValue="3" />
            <!-- 判断上升帧数 -->
            <DownNumVal FirstValue="3" SecondValue="4" ThirdValue="4" FouthValue="3" />
            <!-- 判断下降帧数 -->
            <FindObjValMax FirstValue="1" SecondValue="1" ThirdValue="1" FouthValue="1" />
            <!-- 判断找到目标的大值 -->
            <FindObjValMin FirstValue="1" SecondValue="1" ThirdValue="1" FouthValue="1" />
            <!-- 判断找到目标目标的小值-->
            <WhiteSpotVal FirstValue="100" SecondValue="200" ThirdValue="400" FouthValue="1" />
            <!-- 判断白点数3个区段 -->
            <TopPointVal FirstValue="15" SecondValue="1" ThirdValue="1" FouthValue="1" />
            <!-- 判断Y方向的最大值 -->
            <fFindObjInverse FirstValue="0.600000" SecondValue="0.600000" ThirdValue="0.600000" FouthValue="0.700000" />
            <!-- 判断找到目标和跟踪数的比率 -->
            <fXMatchInverse FirstValue="0.400000" SecondValue="0.400000" ThirdValue="0.600000" FouthValue="0.700000" />
            <!-- 判断 X匹配数与跟踪数的比率 -->
            <fYMatchInverse FirstValue="0.300000" SecondValue="0.300000" ThirdValue="0.600000" FouthValue="0.700000" />
            <!-- 判断 Y匹配数与跟踪数的比率 -->
            <fXContInverse FirstValue="0.500000" SecondValue="0.500000" ThirdValue="0.600000" FouthValue="1.000000" />
            <!-- 判断 X同一方向数与跟踪数的比率 -->
        </ParamDistinguish>
    </camera>
</configure>
